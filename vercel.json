{
  "redirects": [
    {
      "source": "/admin",
      "destination": "/admin/login",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/admin/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
}
